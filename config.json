{
  "compress": false,
  "headers": {
    "access-control-allow-origin": "https://www.ottawa.ski",
    "access-control-allow-headers": "allow,cache-control,content-encoding,content-type,link,strict-transport-security,transfer-encoding,x-frame-options,x-rate-limit,x-rate-remaining,x-rate-reset,x-response-time",
    "access-control-max-age": 60,
    "cache-control": "private, max-age=1800"
  },
  "json": 0,
  "logging": {
    "level": "info",
    "enabled": true,
    "stack": false
  },
  "security": {
    "key": "x-csrf-token",
    "secret": "x",
    "csrf": true,
    "csp": null,
    "xframe": "SAMEORIGIN",
    "p3p": "",
    "hsts": {
      "maxAge": 180,
      "includeSubDomains": true,
      "preload": true
    },
    "xssProtection": true
  },
  "maxBytes": 10240,
  "port": 8000,
  "routes": {},
  "session" : {
    "secret": "x",
    "store": "memory"
  },
  "rate": {
    "enabled": true,
    "limit": 50,
    "reset": 180
  },
  "uid": 33,
  "title": "Ottawa.ski API"
}
